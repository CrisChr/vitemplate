{
  "name": "vitemplate",
  "displayName": "vitemplate",
  "description": "An visual Vite scaffold to generate web project template step by step.",
  "publisher": "BreakingRed",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vitemplate.ViTemplate",
        "title": "ViTemplate"
      }
    ]
  },
  "icon": "public/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrisChr/vitemplate"
  },
  "bugs": {
    "url": "https://github.com/CrisChr/vitemplate/issues"
  },
  "scripts": {
    "package": "pnpm vsce package --no-dependencies",
    "publish": "pnpm vsce publish --no-dependencies",
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
